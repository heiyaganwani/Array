import java.util.ArrayList;
public class Main {
public static void main(String[] args) {
ArrayList<Integer> list = new ArrayList<>();
list.add(20);
list.add(30);
list.add(40);
list.add(50);
System.out.println("Original list: " + list);
list.remove(2);
System.out.println("After removing element at index 2: " + list);
}
}
