import java.util.*;
public class Main {
    public static void main(String[] args) {
        ArrayList<Integer> list = new ArrayList<>();
        list.add(10);
        list.add(20);
        list.add(30);
        list.add(40);
        System.out.println("Before deletion: " + list);
        list.remove(list.size() - 1);
        System.out.println("After deletion: " + list);
    }
}
